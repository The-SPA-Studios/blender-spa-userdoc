    "version": "0.2.0",
    "configurations": [
        {
            "name": "Client (Chrome)",
            "request": "launch",
            "type": "chrome",
            "url": "http://127.0.0.1:8000/",
            "webRoot": "${workspaceFolder}"
        },
        {
            "name": "Server (MkDocs)",
            "type": "python",
            "request": "launch",
            "module": "mkdocs",
            "args": [
                "serve"
            ]
        }
    ],
    "compounds": [
        {
            "name": "Server/Client",
            "configurations": [
                "Client (Chrome)",
                "Server (MkDocs)"
            ]
        }
    ]
}